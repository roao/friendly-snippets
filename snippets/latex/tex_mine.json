{
    "Case_mine":{
        "prefix": "case",
        "body": [
            "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
            "%Case: $1",
            "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
            "\\begin{case}{$1}{${1/([a-zA-Z]+)|([^a-zA-Z]+)/${1:/downcase}${2:+_}/g}$2}",
            "\t\\begin{itemize}",
            "\t\t\\item JobID: $3",
            "\t\t\\item Description: $4",
            "\t\t\\item The parameters are listed in section \\ref{$5}",
            "\t\t\\item $6",
            "\t\\end{itemize}",
            "\\end{case}",
            "$0"
        ],
        "description": "Add a case"
    },
    "Case:Ref": {
        "prefix": ["case:ref", "ca:"],
        "body": ["${1:Case}~\\ref{ca:$2}$0"],
        "description": "Case Reference"
    },
    "SubFigure": {
        "prefix": ["subfigure","sfig"],
        "body": [
            "\\begin{figure}[${1:htbp}]",
            "\t\\begin{center}",
            "\t\\subfloat[]{",
            "\t\t\\includegraphics[width=0.$2\\textwidth]{./figures/$3}",
            "\t\t\\label{sfig:${3/([a-zA-Z]+)|([^a-zA-Z]+)/${1:/downcase}${2:+_}/g}$4}",
            "\t\\subfloat[]{",
            "\t\t\\includegraphics[width=0.$5\\textwidth]{./figures/$6}",
            "\t\t\\label{sfig:${6/([a-zA-Z]+)|([^a-zA-Z]+)/${1:/downcase}${2:+_}/g}$7}",
            "\t\\end{center}",
            "\t\\caption{$8}",
            "\t\\label{fig:$9}",
            "\\end{figure}",
            "$0"
        ],
        "description": "Add a figure"
    },
    "minipage": {
        "prefix": ["minipage","mini"],
        "body": [
            "\\begin{minipage}{0.$1\\textwidth}",
            "\t\\includegraphics[width=0.$2\\linewidth]{./figures/$3}",
            "\\end{minipage}",
            "$0"
        ],
        "description": "Add a minipage"
    },
    "frame": {
        "prefix": ["frame","fr"],
        "body": [
            "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
            "\\begin{frame}",
            "\t$0",
            "\\end{frame}",
            "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%"
        ],
        "description": "Add a minipage"
    },
    "center": {
        "prefix": ["center","ce"],
        "body": [
            "\\begin{center}",
            "\t$0",
            "\\end{center}"
        ],
        "description": "Add a minipage"
    }
}
