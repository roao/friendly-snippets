{
    "()":{
      "prefix": "(",
      "body": "($1)$0"
    },
    "[]":{
      "prefix": "[",
      "body": "[$1]$0"
    },
    "{}":{
      "prefix": "{",
      "body": "{$1}$0"
    },
    "_{}":{
      "prefix": "_",
      "body": "_{$1}$0"
    },
    "^{}":{
      "prefix": "^",
      "body": "^{$1}$0"
    },
    "$$":{
      "prefix": "$",
      "body": "$ $1 $$0"
    },
    "left(  right)":{
      "prefix": "((",
      "body": "\\left( $1 \\right)$0"
    },
    "left[  right]":{
      "prefix": "[[",
      "body": "\\left[ $1 \\right]$0"
    },
    "left<  right>":{
      "prefix": "<<",
      "body": "\\left\\langle $1 \\right\\rangle$0"
    },
    "bar{}":{
      "prefix": ["ba", "bar"],
      "body": "\\bar{$1}$0"
    },
    "sqrt{}":{
      "prefix": "sq",
      "body": "\\sqrt{$1}$0"
    },
    "^{prime}":{
      "prefix": "'",
      "body": "^{\\prime}"
    },
    "hat{}":{
      "prefix": "^",
      "body": "\\hat{$1}$0"
    },
    "Case_mine":{
        "prefix": "case",
        "body": [
            "\\begin{case}{$1}{${1/([a-zA-Z]+)|([^a-zA-Z]+)/${1:/downcase}${2:+_}/g}$2}",
            "\t\\begin{itemize}",
            "\t\t\\item JobID: $3",
            "\t\t\\item Description: $4",
            "\t\t\\item The parameters are listed in section \\ref{$5}",
            "\t\t\\item $6",
            "\t\\end{itemize}",
            "\\end{case}",
            "$0"
        ],
        "description": "Add a case"
    },
    "Case:Ref": {
        "prefix": ["case:ref", "ca:"],
        "body": ["${1:Case}~\\ref{ca:$2}$0"],
        "description": "Case Reference"
    },
    "SubFigure": {
        "prefix": ["subfigure","sfig"],
        "body": [
            "\\begin{figure}[${1:htbp}]",
            "\t\\begin{center}",
            "\t\\subfloat[]{",
            "\t\t\\includegraphics[width=0.$2\\textwidth]{./figures/$3}",
            "\t\t\\label{sfig:${3/([a-zA-Z]+)|([^a-zA-Z]+)/${1:/downcase}${2:+_}/g}$4}}",
            "\t\\subfloat[]{",
            "\t\t\\includegraphics[width=0.$5\\textwidth]{./figures/$6}",
            "\t\t\\label{sfig:${6/([a-zA-Z]+)|([^a-zA-Z]+)/${1:/downcase}${2:+_}/g}$7}}",
            "\t\\end{center}",
            "\t\\label{fig:$8}",
            "\t\\caption{$9}",
            "\\end{figure}",
            "$0"
        ],
        "description": "Add a figure"
    },
